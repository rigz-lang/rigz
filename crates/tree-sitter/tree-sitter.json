{
  "grammars": [
    {
      "name": "rigz",
      "camelcase": "Rigz",
      "scope": "source.rigz",
      "path": ".",
      "file-types": [
        "rigz",
        "rg"
      ],
      "highlights": "queries/highlights.scm",
      "injection-regex": "rigz"
    }
  ],
  "metadata": {
    "version": "0.3.0",
    "license": "MIT",
    "description": "Rigz grammar for tree-sitter",
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-rigz"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}